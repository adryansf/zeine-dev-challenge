{
  "name": "backend",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "c8 node --test --env-file=.env --experimental-strip-types --no-warnings \"test/**/*.ts\"",
    "start": "node --experimental-strip-types --no-warnings src/server.ts",
    "dev": "node --env-file=.env --experimental-strip-types --watch --no-warnings src/server.ts",
    "migrate": "drizzle-kit migrate",
    "migrate_and_start": "npm run migrate && npm run start"
  },
  "keywords": [],
  "author": "Adryan Freitas",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/cors": "^11.0.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "better-auth": "^1.2.12",
    "dotenv": "^17.2.0",
    "drizzle-orm": "^0.44.3",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.0",
    "fastify-type-provider-zod": "^5.0.2",
    "minio": "^8.0.5",
    "pg": "^8.16.3",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "@types/pg": "^8.15.4",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "drizzle-kit": "^0.31.4",
    "fastify-tsconfig": "^3.0.0",
    "ts-node": "^10.4.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.2"
  }
}
